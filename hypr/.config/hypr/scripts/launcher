#!/bin/bash

CONFIG="$HOME/.config/hypr/modules/wofi/config"
STYLE="$HOME/.config/hypr/modules/wofi/style.css"
COLORS="$HOME/.config/hypr/modules/wofi/colors"

if [[ ! $(pidof wofi) ]]; then
	wofi --show drun --prompt 'Search...' --conf ${CONFIG} --style ${STYLE} --color ${COLORS}
else
	pkill wofi
fi
